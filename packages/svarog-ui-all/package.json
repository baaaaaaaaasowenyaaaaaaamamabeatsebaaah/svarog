{
  "name": "svarog-ui-all",
  "version": "2.3.0",
  "description": "Complete Svarog UI library with all components and themes in one bundle",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "browser": "dist/svarog-ui-all.js",
  "unpkg": "dist/svarog-ui-all.js",
  "jsdelivr": "dist/svarog-ui-all.js",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "browser": "./dist/svarog-ui-all.js"
    },
    "./css": "./dist/styles.css",
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "echo 'Build is handled by monorepo root'",
    "prepublishOnly": "cd ../.. && npm run build:all-package"
  },
  "keywords": [
    "svarog-ui",
    "components",
    "ui",
    "vanilla-js",
    "bundle",
    "complete",
    "all-in-one"
  ],
  "author": "Sebastian Huber",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baaaaaaaaasowenyaaaaaaamamabeatsebaaah/svarog.git",
    "directory": "packages/svarog-ui-all"
  },
  "sideEffects": [
    "*.css",
    "./dist/styles.css"
  ],
  "gitHead": "af294650dbf40f793aa367ba2f27cdfc8c33ba6b"
}
